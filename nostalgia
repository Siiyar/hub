if game.placeId == 487316 then
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Fuzki-UI-Library/main/FuzkiLibrary.lua"))() -- Gets the Library
local Main = Library:Create("Nostalgia Hub", "Crossroad") -- Creates Main Window | Fuzki (Hub Name) | Baseplate (Game Name)

local Combat = Main:CreateSection("Combat")
local Misc = Main:CreateSection("Misc.")
local Preview = Main:CreateSection("Preview")



local cheater = game:GetService("Players").LocalPlayer



Combat:CreateLabel("Combat cheats.")
Combat:CreateToggle("Toggle Info", function(state) 
    local sword = "Sword"
    _G.autoEq = state

    while wait() and _G.autoEq == true do
        pcall(function()
            if cheater.Backpack:FindFirstChild(sword) and cheater.Character:FindFirstChild(sword) == nil then
                local tool = cheater.Backpack:FindFirstChild(sword)
                cheater.Character.Humanoid:EquipTool(tool)
            end
        end)
    end
end)

Combat:CreateButton("Click me browski", "Loopkill all", function()
    local args = {
    [1] = "(Nostalgia Hub) lol im a loser and got kicked by a troll feature",
    [2] = "All"
    }

    game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
    wait(0.1)
    cheater:Kick("get out loser")
end)

--- Misc
Misc:CreateButton("Check", "Check your Executor Level", function() -- creates button (CreateButton)
    local args = {
    [1] = "My executor Level is "..Identity(),
    [2] = "All"
    }

    game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
end)


-- Notification (literally use it idgaf)
local notification = Instance.new("Sound")

notification.Parent = game:GetService("SoundService")
notification.SoundId = "rbxassetid://9086208751"
notification.Volume = 5
notification.Name = "Nostalgia Hub"
notification:Play()
