if not game:IsLoaded() then
    game.Loaded:wait()
end

if game.placeId == 487316 then
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Fuzki-UI-Library/main/FuzkiLibrary.lua"))()
local Main = Library:Create("Nostalgia Hub", "SFOTH")


-- Sections
local Combat = Main:CreateSection("Combat")
local Client = Main:CreateSection("Client")
local Misc = Main:CreateSection("Misc.")

--variables
local cheater = game.Players.LocalPlayer


-- Combat
Combat:CreateLabel("nothing yet lol")

--- Client
Client:CreateSlider(16, 300, "Walkspeed", function(val)
    cheater.Character.Humanoid.WalkSpeed = val
end)

Client:CreateSlider(16, 300, "Jumppower", function(val)
    cheater.Character.Humanoid.JumpPower = val
end)


Combat:CreateToggle("Anti Heal", function(state)
    getgenv().antiHeal = state

    spawn(function()
            while antiHeal == true do
            cheater.Character.Humanoid.Sit = true
            cheater.Character.HumanoidRootPart.CFrame = CFrame.new(-124, 252.5, 4, -1, 0, 0, 0, 1, 0, 0, 0, -1)
            task.wait(0.05)
            cheater.Character.HumanoidRootPart.CFrame = CFrame.new(-1, -7, -13)
            task.wait(0.05)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(136, 243.5, 2, -1, 0, 0, 0, 1, 0, 0, 0, -1)
            task.wait(0.05)
            cheater.Character.HumanoidRootPart.CFrame = CFrame.new(-1, -7, -13)
            task.wait()
        end
    end)

    if cheater.Character.Humanoid.Health == 0 then
        antiHeal = false
    elseif antiHeal == false then
        cheater.Character.HumanoidRootPart.CFrame = CFrame.new(5, 248, -145)
        wait()
        cheater.Character.Humanoid.Sit = false
    end
end)

--- Misc
Misc:CreateButton("Apply", "Apply Anti chat logger", function()
    -- credits to AnthonyIsntHere
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AnthonyIsntHere/anthonysrepository/main/scripts/AntiChatLogger.lua"))()
end)

-- detection
loadstring(game:HttpGet("https://pastebin.com/raw/1gv8s32s"))()



end
