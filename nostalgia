        Icon = "rbxassetid://9524079134";
        Duration = 10; 
        Button1 = "thanks bbg";
    })
elseif Valyse then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Nostalgia Hub";
        Text = "Hello Valyse user, have fun! Made by @siiyar";
        Icon = "";
        Duration = 10; 
        Button1 = "thanks bbg";
    })
end
