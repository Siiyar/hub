if not game:IsLoaded() then
    game.Loaded:wait()
end

if game.placeId == 487316 then
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Fuzki-UI-Library/main/FuzkiLibrary.lua"))()
local Main = Library:Create("Nostalgia Hub", "SFOTH")

-- Sections
local Combat = Main:CreateSection("Combat")
local Player = Main:CreateSection("Local Player")
local Misc = Main:CreateSection("Misc.")
local Preview = Main:CreateSection("Preview")

-- just local variables
local cheater = game:GetService("Players").LocalPlayer

-- Combat
Combat:CreateLabel("Combat cheats.")
Combat:CreateToggle("Toggle Info", function(state) 
    local sword = "Sword"
    _G.autoEq = state

    while wait() and _G.autoEq == true do
        pcall(function()
            if cheater.Backpack:FindFirstChild(sword) and cheater.Character:FindFirstChild(sword) == nil then
                local tool = cheater.Backpack:FindFirstChild(sword)
                cheater.Character.Humanoid:EquipTool(tool)
            end
        end)
    end
end)

Combat:CreateButton("Click me browski", "Loopkill all", function()
    local args = {
    [1] = "(Nostalgia Hub) lol im a loser and got kicked by a troll feature",
    [2] = "All"
    }

    game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
    wait(0.1)
    cheater:Kick("get out loser")
end)

--- Player
Player:CreateButton("Apply", "Get 200 HP", function()
    print("sex")
end)

Player:CreateToggle("Anti Heal", function(state)
    getgenv().antiHeal = state

    while antiHeal == true do
        cheater.Character.Humanoid.Sit = true
        firetouchinterest(game:GetService("Workspace").HealPad.Pad, game.Players.LocalPlayer.Character.HumanoidRootPart, 0)
        firetouchinterest(game:GetService("Workspace").HealPad.Pad, game.Players.LocalPlayer.Character.HumanoidRootPart, 1)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(136, 246.599991, 2, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        wait()
    end
end)


Player:CreateToggle("Anti Heal V2 BROKEN", function(state)
    getgenv().antiHeal2 = state
    
    while antiHeal2 == true do
        game.Players.LocalPlayer.Character.LeftArm.CFrame = CFrame.new(-124, 254.999985, 4, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(136, 246.599991, 2, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        wait()
    end
end)

-- i will do a second version at some point

--- Misc
Misc:CreateButton("Apply", "Apply Anti chat logger", function() -- creates button (CreateButton)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AnthonyIsntHere/anthonysrepository/main/scripts/AntiChatLogger.lua"))()
end)


